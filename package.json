{
  "name": "react-electron-vite",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "main": "src/main.js",
  "scripts": {
    "start": "npm-run-all -p electron:serve react:serve",
    "build": "npm run react:build && npm run electron:build",
    "react:serve": "vite --port 4200",
